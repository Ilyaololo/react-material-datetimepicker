{
  "presets": [
    "react", 
    "es2015", 
    "es2016", 
    "stage-0", 
    "stage-2", 
    "stage-3"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-async-generator-functions",
    [
      "transform-runtime", {
        "polyfill": true,
        "regenerator": true
      }
    ]
  ]
}
